<?xml version="1.0"?>
<launch>


  <node name="lowlevel_ctrl" pkg="lowlevel_ctrl" type="lowlevel_ctrl" output="screen">      
  </node>
  
       <node name="left_wheel_pid" pkg="pid" type="controller" output="screen" >
      <param name="Kp" value="6.0" />
      <param name="Ki" value="0.005" />
      <param name="Kd" value="0.1" />
      <param name="upper_limit" value="20" />
      <param name="lower_limit" value="-20" />
      <param name="windup_limit" value="10" />
      <param name="cutoff_frequency" value="20" />
      <param name="max_loop_frequency" value="105.0" />
      <param name="min_loop_frequency" value="95.0" />
      <param name="setpoint_timeout" value="-1.0" />
     </node>

</launch>
